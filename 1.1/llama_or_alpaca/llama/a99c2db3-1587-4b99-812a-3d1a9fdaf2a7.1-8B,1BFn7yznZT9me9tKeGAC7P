    <script>
// TODO: check if it's obsolete
$('[data-toggle="collapse"]').on('click', function() {
  let item = $(this);
  item.find('i').toggleClass('fa-plus-circle fa-minus-circle');
});


// ----------------------------------------------------------------------------------------------------------
function handle_url_copy() {
    new ClipboardJS('#copy_url', {
        text: function(trigger) {
            let o = document.getElementById('copied_ok');
            o.style.display = 'inline';
            setTimeout(function() {
                o.style.display = 'none';
            }, 2000);
            return trigger.getAttribute('content');
        }
    });
}

// ----------------------------------------------------------------------------------------------------------
// the reaction block
const react_obj_list = ['1BFn7yznZT9me9tKeGAC7P_instruct','1BFn7yznZT9me9tKeGAC7P_factual','1BFn7yznZT9me9tKeGAC7P_censor','1BFn7yznZT9me9tKeGAC7P_dataanal','1BFn7yznZT9me9tKeGAC7P_textgen','1BFn7yznZT9me9tKeGAC7P_textsum','1BFn7yznZT9me9tKeGAC7P_codegen','1BFn7yznZT9me9tKeGAC7P_multilang'];
const model_uid = '1BFn7yznZT9me9tKeGAC7P';

function generateSimpleStars(avgRating, disabled = true) {
    let html = '';
    let starsCount = 0;

    // Map the avgRating to the number of stars
    if (avgRating >= 0 && avgRating <= 1.0) {
        starsCount = 1;
    } else if (avgRating > 1.0 && avgRating <= 2.0) {
        starsCount = 2;
    } else if (avgRating > 2.0 && avgRating <= 3.0) {
        starsCount = 3;
    } else if (avgRating > 3.0 && avgRating <= 4) {
        starsCount = 4;
    }

    // Generate stars HTML based on starsCount
    for (let i = 0; i < starsCount; i++) {
        if (disabled) {
            html += '<i class="mr-1 stars_disabled">●</i>';
        } else {
            html += '<i class="mr-1 stars_' + starsCount + '">●</i>';
        }
    }

    // Ensure the total count is 4 by adding empty stars if necessary
    for (let i = starsCount; i < 4; i++) {
        html += '<i class="mr-1" class="stars_disabled">●</i>';
    }

    return html;
}

// ----------------------------------------------------------------------------------------------------------
function updateLikes(status, data, action = '') {
    let like_obj = document.getElementById('like_count')
    let likes_count = Number.parseFloat(like_obj.getAttribute('data-likes'));
    like_obj.innerText = likes_count + "";

    if (!status || !data['stats'] || data['stats'].length == 0) {
        return;
    }

    // single like was removed
    if (data['stats'][0]['deleted']) {
        return;
    }

    // increment the number of likes by the number of votes
    like_obj.innerText = (Number.parseFloat(data['stats'][0]['num_r']) + likes_count) + "";
}

// ----------------------------------------------------------------------------------------------------------
function updateStars(status, data, action = '') {
    if (!status || !data['stats'] || data['stats'].length == 0) {
        return;
    }

    let stats = data['stats'];

    // iterate thru the lst of objects and t=find those which have votes, otherwise clear them
    for (let j = 0; j < stats.length; j++) {
        if (stats[j]['deleted']) {
            document.getElementById('res_' + stats[j]['r_obj']).innerHTML = '<span class="stars_disabled">' + generateSimpleStars(0.0, true) + '</span>';
        } else {
            document.getElementById('res_' + stats[j]['r_obj']).innerHTML = '<span class="stars_disabled">' + generateSimpleStars(stats[j]['avg_r'], false) + '</span>';
        }
    }
}

// ----------------------------------------------------------------------------------------------------------
document.addEventListener('DOMContentLoaded', function() {
    disableButtons('model_cap_rank', react_obj_list, true);

    for (let i = 0; i < react_obj_list.length; i++) {
        // clear stars if the object absent
        document.getElementById('res_' + react_obj_list[i]).innerHTML = '<span class="stars_disabled">' + generateSimpleStars(0.0, true) + '</span>';
    }

    // load the stars
    load_reacts('model_cap_rank', react_obj_list, function(status, data) {
        disableButtons('model_cap_rank', react_obj_list, false);
        updateStars(status, data);
    });

    disableButtons('model_likes', model_uid, true);

    // load the likes
    load_reacts('model_likes', model_uid, function(status, data) {
        disableButtons('model_likes', model_uid, false);
        updateLikes(status, data);
    });

    loadScript('/js/clipboard.min.js', function(){
        handle_url_copy();
    });
});

// ----------------------------------------------------------------------------------------------------------
</script>



